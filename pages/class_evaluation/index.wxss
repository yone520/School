/* pages/my_help_center/index.wxss */
.t1 {
  height: 400rpx;
  background-color: #fff;
  padding: 32rpx;
  margin-top: 20rpx;
}

.t1 textarea {
  width: 100%;
  font-size: 32rpx;
  height: 300rpx;
}

.t1 .ct {
  text-align: right;
  color: rgba(47, 52, 68, .6);
  font-size: 32rpx;
}

.uploadimg {
  background-color: #fff;
  margin-top: 20rpx;
  padding: 0 32rpx;
  margin-bottom: 154rpx;
}

.tit {
  color: rgba(47, 52, 68, 1);
  font-size: 32rpx;
  padding-top: 32rpx;
}

.addimg image {
  width: 100%;
  height: 100%;
}

.addimg {
  overflow: hidden;
  border: 1rpx solid #bbbbbb;
  width: 180rpx;
  height: 180rpx;
  border-radius: 12rpx;
  background: url("data:image/png;base64,") no-repeat center/44rpx;
  margin-right: 72rpx;
  margin-bottom: 32rpx;
}

.imgcontent {
  padding: 60rpx 0 30rpx 0;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
}

.imgcontent .addimg:nth-child(3n) {
  margin-right: 0;
}

.cantactphone {
  display: flex;
  height: 100rpx;
  padding: 0 32rpx;
  align-items: center;
  background-color: #fff;
  margin-top: 20rpx;
}

.cantactphone > text {
  color: rgba(36, 42, 58, 1);
  font-size: 32rpx;
  width: 180rpx;
}

.cantactphone input {
  font-size: 32rpx;
  color: #242A3A;
  flex: 1;
}

.kecmc {
  height: 100rpx;
  line-height: 100rpx;
  color: rgba(28, 31, 32, 1);
  font-size: 32rpx;
  text-align: center;
}

.top1 {
  background-color: #fff;
}

.xinghao {
  display: flex;
  justify-content: center;
  color: rgba(28, 31, 32, 1);
  font-size: 28rpx;
  align-items: center;
}

.xinghao image {
  width: 43.8rpx;
  margin: 0 15rpx;
}

.andj {
  color: rgba(28, 31, 32, 1);
  font-size: 28rpx;
  text-align: center;
}

.xinghao text {
  margin-left: 25rpx;
}

.pjial {
  height: 422rpx;
  padding: 40rpx 0;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}